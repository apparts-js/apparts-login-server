{
  "name": "@apparts/login-server",
  "version": "2.0.0",
  "description": "Login and Signup for Web, Apps, Server",
  "main": "index.js",
  "scripts": {
    "test:one": "jest",
    "test": "jest --watch --detectOpenHandles",
    "testCoverage": "jest --coverage",
    "serve": "node ./server/app.js",
    "setupDB": "node ./server/scripts/setup.js",
    "prepublishOnly": "npm run clean && npm ci  && npm run ci && npm run build",
    "build": "npm run clean",
    "ci": "npm ci && npm run ci:prettier && npm run ci:eslint && npm run ci:test",
    "ci:prettier": "CI=true prettier --check .",
    "ci:test": "CI=true npm run test:one",
    "ci:eslint": "eslint . --ext .js,.jsx,.ts,.tsx -c .eslintrc.ci.js",
    "clean": "rimraf coverage build tmp"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phuhl/apparts-login-server.git"
  },
  "keywords": [
    "apparts",
    "app",
    "server",
    "node",
    "login",
    "signup"
  ],
  "author": "Philipp Uhl",
  "license": "MIT",
  "dependencies": {
    "@apparts/config": "^1.0.6",
    "@apparts/db": "^2.1.1",
    "@apparts/error": "^1.1.0",
    "@apparts/model": "^4.0.2",
    "@apparts/types": "^6.1.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@apparts/backend-test": "^1.2.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "jest": "^27.5.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  },
  "optionalPeerDependencies": {
    "apparts-node-app": "git+ssh://git@ph-uhl.com:Apparts/apparts-node-app#semver:^1.0.0"
  }
}
